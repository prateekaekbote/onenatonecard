{
  "version": 2,
  "builds": [
    {
      "src": "api/issuer_server.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/styles.css",
      "dest": "styles.css"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/issuer_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}